{
  "main": "app/Entrypoint.tsx",
  "homepage": "https://openmanifest.org",
  "scripts": {
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --ci --collectCoverage=true --passWithNoTests --testResultsProcessor=jest-junit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint app/",
    "lint:fix": "eslint app/ --fix",
    "lint:ci": "eslint app/ --format junit",
    "ts:graphql": "curl http://127.0.0.1:5000/graphql -o schema.graphql; yarn graphql-codegen",
    "start:development": "expo start",
    "start:production": "EXPO_ENV=production expo start",
    "start:staging": "EXPO_ENV=staging expo start",
    "web:start:dev": "EXPO_ENV=development expo start --web",
    "web:start:staging": "EXPO_ENV=staging expo start --web",
    "web:start:prod": "EXPO_ENV=production expo start --web",
    "web:build:staging": "EXPO_ENV=staging expo build:web -d && mkdir -p web-build-staging && echo 'staging.openmanifest.org' > web-build-staging/CNAME && cp -rf ./web-build/* web-build-staging/ && cp web-build-staging/index.html web-build-staging/404.html && cp apple-app-site-association.json web-build-staging/",
    "web:build:production": "EXPO_ENV=production expo build:web && mkdir -p web-build-prod && echo 'staging.openmanifest.org' > web-build-prod/CNAME && cp -rf ./web-build/* web-build-prod/ && cp web-build-prod/index.html web-build-prod/404.html && cp apple-app-site-association.json web-build-prod/",
    "web:upload:production": "gh-pages -d web-build-prod --repo https://github.com/OpenManifest/openmanifest-web.git",
    "web:upload:staging": "gh-pages -d web-build-staging --repo https://github.com/OpenManifest/openmanifest-web-staging.git",
    "web:deploy:staging": "yarn web:build:staging && yarn web:upload:staging",
    "web:deploy:production": "yarn web:build:production && yarn web:upload:production",
    "ios:start:dev": "EXPO_ENV=development expo start --ios",
    "ios:start:prod": "EXPO_ENV=production expo start --ios",
    "ios:build:staging": "EXPO_ENV=staging expo build:ios -t archive --skip-credentials-check",
    "ios:build:prod": "EXPO_ENV=production expo build:ios -t archive --skip-credentials-check --release-channel production",
    "android:start:dev": "EXPO_ENV=development expo start --android",
    "android:start:prod": "EXPO_ENV=production expo start --android",
    "android:build": "EXPO_ENV=production expo build:android",
    "android:build:staging": "EXPO_ENV=staging expo build:android",
    "expo:publish:staging": "EXPO_ENV=staging expo publish",
    "expo:publish:prod": "EXPO_ENV=production expo publish --release-channel production",
    "predeploy": "expo build:web"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry|react-native-iphone-x-helper/.*)"
    ],
    "testPathIgnorePatterns": [
      "/__mocks__/.*.mock.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./node_modules/react-native/jest/setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/jest.setup.ts"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/__mocks__/**"
    ]
  },
  "resolutions": {
    "react-native-reanimated": "~2.2.0",
    "react-native-redash": "16.1.1",
    "expo-linear-gradient": "~9.2.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
