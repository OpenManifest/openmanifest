{
  "main": "app/Entrypoint.tsx",
  "homepage": "https://openmanifest.org",
  "scripts": {
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --ci --collectCoverage=true --passWithNoTests --testResultsProcessor=jest-junit",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint -c .eslintrc.js app/",
    "lint:fix": "eslint -c .eslintrc.js app/ --fix",
    "lint:ci": "eslint -c .eslintrc.js app/ --format junit",
    "gql:schema:staging": "curl https://devapi.openmanifest.org/graphql -o schema.graphql",
    "gql:schema:production": "curl https://api.openmanifest.org/graphql -o schema.graphql",
    "gql:schema:local": "curl http://127.0.0.1:5000/graphql -o schema.graphql",
    "ts:graphql:staging": "yarn gql:schema:staging && yarn graphql-codegen",
    "ts:graphql:production": "yarn gql:schema:production && yarn graphql-codegen",
    "ts:graphql:local": "yarn gql:schema:local && yarn graphql-codegen",
    "start:development": "expo start",
    "start:production": "EXPO_ENV=production expo start",
    "start:staging": "EXPO_ENV=staging expo start",
    "web:start:dev": "EXPO_ENV=development expo start --web",
    "web:start:staging": "EXPO_ENV=staging expo start --web",
    "web:start:prod": "EXPO_ENV=production expo start --web",
    "web:build:staging": "EXPO_ENV=staging expo build:web -d && mkdir -p web-build-staging && echo 'staging.openmanifest.org' > web-build-staging/CNAME && cp -rf ./web-build/* web-build-staging/ && cp web-build-staging/index.html web-build-staging/404.html && cp apple-app-site-association.json web-build-staging/ && mkdir -p web-build-staging/.well-known && cp apple-app-site-association.json web-build-staging/.well-known/",
    "web:build:production": "EXPO_ENV=production expo build:web && mkdir -p web-build-prod && echo 'openmanifest.org' > web-build-prod/CNAME && cp -rf ./web-build/* web-build-prod/ && cp web-build-prod/index.html web-build-prod/404.html && cp apple-app-site-association.json web-build-prod/ && mkdir -p web-build-prod/.well-known && cp apple-app-site-association.json web-build-prod/.well-known/",
    "web:upload:production": "gh-pages -t -d web-build-prod --repo https://github.com/OpenManifest/openmanifest-web.git",
    "web:upload:staging": "gh-pages -t -d web-build-staging --repo https://github.com/OpenManifest/openmanifest-web-staging.git",
    "web:deploy:staging": "yarn web:build:staging && yarn web:upload:staging",
    "web:deploy:production": "yarn web:build:production && yarn web:upload:production",
    "ios:start:dev": "EXPO_ENV=development expo start --ios",
    "ios:start:prod": "EXPO_ENV=production expo start --ios",
    "ios:build:simulator": "EXPO_ENV=staging expo build:ios -t simulator --skip-credentials-check",
    "ios:build:staging": "EXPO_ENV=staging expo build:ios -t archive",
    "ios:build:prod": "EXPO_ENV=production expo build:ios -t archive --skip-credentials-check --release-channel production",
    "android:start:dev": "EXPO_ENV=development expo start --android",
    "android:start:prod": "EXPO_ENV=production expo start --android",
    "android:build": "EXPO_ENV=production expo build:android --release-channel production",
    "android:build:staging": "EXPO_ENV=staging expo build:android",
    "android:build:prod": "EXPO_ENV=production expo build:android --release-channel production",
    "expo:publish:staging": "EXPO_ENV=staging expo publish",
    "expo:publish:prod": "EXPO_ENV=production expo publish --release-channel production",
    "predeploy": "expo build:web"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?(@react-native|expo-modules-core|react-native)|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry|react-native-iphone-x-helper|react-native-reanimated|expo-linear-gradient/.*)"
    ],
    "testPathIgnorePatterns": [
      "/__mocks__/.*.mock.ts",
      "/__fixtures__/.*.fixture.ts",
      "app/__tests__/manifest/ManifestScreen.test.tsx"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./node_modules/react-native/jest/setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/jest.setup.ts"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/__mocks__/**"
    ]
  },
